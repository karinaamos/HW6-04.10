#include <stdio.h>

int main(){
    int a,b,c,d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    if ((b == c) && (c == d)){
        printf("1");
    }
    if ((a == c) && (c == d)){
        printf("2");
    }
    if ((a == b) && (b == d)){
        printf("3");
    }
    if ((a == b) && (b == c)){
        printf("4");
    }
    return 0;
}
